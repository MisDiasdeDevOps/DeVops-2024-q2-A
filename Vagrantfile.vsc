Vagrant.configure("2") do |config|
  config.vm.define "vm-marcelo-A-cordoba" do |vm|
    vm.box = "debian/buster64"
    vm.hostname = "vm-marcelo-A-cordoba"
    vm.network "private_network", type: "dhcp"
  end

  config.vm.provider "virtualbox" do |vb|
    vb.gui = true
    vb.memory = "8192"
  end
end


